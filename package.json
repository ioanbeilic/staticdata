{
  "name": "static-data",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs-plus/common": "^1.1.0",
    "@nestjs-plus/rabbitmq": "^1.3.0",
    "@nestjs/common": "^6.8.1",
    "@nestjs/core": "^6.8.1",
    "@nestjs/elasticsearch": "^6.0.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^3.1.0",
    "@types/elasticsearch": "^5.0.35",
    "@types/mongoose": "^5.5.19",
    "amqp-connection-manager": "^3.0.0",
    "amqplib": "^0.5.5",
    "bluebird": "^3.7.1",
    "class-transformer": "^0.2.3",
    "crypto-js": "^3.1.9-1",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.4.1",
    "dotenv": "^8.1.0",
    "elasticsearch": "^16.4.0",
    "fast-xml-parser": "^3.13.0",
    "form-data": "^2.5.1",
    "he": "^1.2.0",
    "iconv-lite": "^0.5.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.3",
    "mysql": "^2.17.1",
    "nest-winston": "^1.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sqlite3": "^4.1.0",
    "ssh2-sftp-client": "^4.2.1",
    "swagger-ui-express": "^4.1.1",
    "typy": "^3.3.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.8.1",
    "@types/amqplib": "^0.5.13",
    "@types/bluebird": "^3.5.28",
    "@types/crypto-js": "^3.1.43",
    "@types/csvtojson": "^1.1.5",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/form-data": "^2.5.0",
    "@types/he": "^1.1.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.11",
    "@types/ssh2-sftp-client": "^4.1.0",
    "@types/supertest": "^2.0.8",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "querystring": "^0.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsc-watch": "^4.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.0-beta"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  }
}
